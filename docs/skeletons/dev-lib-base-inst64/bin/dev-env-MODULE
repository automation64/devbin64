# X_GLOBALS_PLACEHOLDER_X
{
  # Lib / X_MODULE_TITLE_X / Installer
  export DEV_LIB_X_MODULE_UPPER_X_INSTALLER="${DEV_LIB_INSTALLER64_TARGET}/install-X_INSTALLER_X"
  # Lib / X_MODULE_TITLE_X / Target
  export DEV_LIB_X_MODULE_UPPER_X_TARGET="${DEV_LIB_X_MODULE_UPPER_X_TARGET:-${DEV_PATH_LIB}/X_DESTINATION_X}"
}

# X_LOCALS_PLACEHOLDER_X
{
  # Lib / X_MODULE_TITLE_X / Skip installation?
  export DEV_LIB_X_MODULE_UPPER_X_SKIP='YES'
  # Lib / X_MODULE_TITLE_X / Command
  export DEV_LIB_X_MODULE_UPPER_X_BIN="${DEV_LIB_X_MODULE_UPPER_X_BIN:-}"
  if [[ -z "$DEV_LIB_X_MODULE_UPPER_X_BIN" ]]; then
    DEV_LIB_X_MODULE_UPPER_X_BIN="$(bl64_bsh_command_locate_user 'X_COMMAND_X')"
    if [[ -z "$DEV_LIB_X_MODULE_UPPER_X_BIN" ]]; then
      DEV_LIB_X_MODULE_UPPER_X_BIN="${DEV_LIB_X_MODULE_UPPER_X_TARGET}/X_COMMAND_X"
      DEV_LIB_X_MODULE_UPPER_X_SKIP='NO'
    fi
  fi
}
